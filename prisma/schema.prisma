datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Artist {
  id    String     @default(uuid()) @id
  name String
  grammy Boolean
}

model User {
  id    String      @default(uuid()) @id
  login String
  password String
  updatedAt Int
  createdAt Int
  version Int
}